<h1>New visitor</h1>

<% form_for @visitor, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  

  <p>
    <%= f.label :caption %><br />
    <%= f.text_field :caption %>
  </p>
  <p>   
    <%= f.label :photo %><br>
    <%= f.file_field :photo %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', visitors_path %>